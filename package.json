{
  "name": "posquelize",
  "version": "0.0.1",
  "publishConfig": {
    "tag": "latest"
  },
  "description": "Automatically generate bare sequelize models, migrations and sequelize app from your database.",
  "main": "index.js",
  "types": "types",
  "keywords": [
    "typescript",
    "scaffold",
    "generator",
    "postgres",
    "sequelize",
    "sequelizejs",
    "mapper",
    "boilerplate",
    "migration",
    "repository",
    "jsonb",
    "minimal-app"
  ],
  "files": [
    "*.js",
    "lib/**/*.js",
    "lib/**/*.sql",
    "lib/**/*.njk",
    "types/**/*.ts",
    "*.md"
  ],
  "bin": {
    "posquelize": "bin/posquelize"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blacksmoke26/posquelize.git"
  },
  "bugs": {
    "url": "https://github.com/blacksmoke26/posquelize/issues"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf lib && rimraf types",
    "copy:sqls": "copyfiles -u 1 src/sqls/**/* lib/",
    "copy:templates": "copyfiles -u 1 src/templates/**/* lib/",
    "build": "npm run clean && npm run copy:sqls && npm run copy:templates && tsc && tsc-alias",
    "prepack": "crlf --set=LF bin/posquelize",
    "repl": "babel-node ./src/repl.ts --extensions .ts,.js"
  },
  "engines": {
    "node": ">= 20"
  },
  "author": "Junaid Atari (https://github.com/blacksmoke26)",
  "license": "ISC",
  "dependencies": {
    "@dbml/connector": "^5.1.0",
    "@dbml/core": "^5.1.0",
    "change-case": "^5.4.4",
    "copyfiles": "^2.4.1",
    "crlf": "^1.1.1",
    "deepmerge": "^4.3.1",
    "figlet": "^1.9.3",
    "knex": "^3.1.0",
    "moment": "^2.30.1",
    "nunjucks": "^3.2.4",
    "pluralize": "^8.0.0",
    "rimraf": "^6.1.0",
    "sprintf-js": "^1.1.3",
    "sql-formatter": "^15.6.10",
    "yargs": "^18.0.0"
  },
  "peerDependencies": {
    "sequelize": "^6.14.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/node": "^7.28.0",
    "@babel/plugin-proposal-async-do-expressions": "^7.27.1",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-proposal-do-expressions": "^7.28.3",
    "@babel/plugin-proposal-export-default-from": "^7.27.1",
    "@babel/plugin-proposal-throw-expressions": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@types/node": "^24.10.0",
    "@types/nunjucks": "^3.2.6",
    "@types/pluralize": "^0.0.33",
    "@types/sprintf-js": "^1.1.4",
    "@types/yargs": "^17.0.34",
    "babel-plugin-module-resolver": "^5.0.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsc-watch": "^7.2.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
